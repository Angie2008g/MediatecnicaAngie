# Función para imprimir la tabla de multiplicar de un número
def imprimir_tabla_multiplicar(n):
    print(f"Tabla de multiplicar del {n}:")
    for i in range(1, 11):
        print(f"{n} x {i} = {n * i}")
    print()

# Imprimir las tablas de multiplicar del 1 al 10
for numero in range(1, 11):
    imprimir_tabla_multiplicar(numero)
